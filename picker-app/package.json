{
  "name": "picker-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn codegen && next dev",
    "build": "yarn codegen && next build && yarn sitemap",
    "export": "yarn codegen && next build && next export",
    "start": "yarn codegen && next start",
    "lint": "next lint",
    "fix": "yarn lint --fix",
    "lint:css": "stylelint ./src/**/*.{css,js,jsx,ts,tsx}",
    "fix:css": "stylelint ./src/**/*.{css,js,jsx,ts,tsx} --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "codegen": "yarn workspace picker-types codegen && yarn codegen:nexus && yarn codegen:graphql",
    "codegen:nexus": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only ./src/graphql/nexus-schema.ts",
    "codegen:graphql": "graphql-codegen --config src/graphql/codegen.ts",
    "sitemap": "next-sitemap --config next-sitemap.js"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.3",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-aspect-ratio": "1.0.0",
    "@radix-ui/react-checkbox": "^1.0.0",
    "@radix-ui/react-label": "^1.0.0",
    "@radix-ui/react-scroll-area": "^1.0.1",
    "@radix-ui/react-separator": "^1.0.0",
    "@radix-ui/react-slider": "^1.0.0",
    "@tanstack/react-query": "^4.10.3",
    "apollo-server-micro": "^3.10.3",
    "chart.js": "^3.9.1",
    "chartjs-plugin-datalabels": "^2.1.0",
    "graphql": "^16.6.0",
    "lodash-es": "^4.17.21",
    "micro": "^9.4.1",
    "next": "13.0.1",
    "next-seo": "^5.11.1",
    "nexus": "^1.3.0",
    "react": "18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.38.0",
    "recoil": "^0.7.6",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.2",
    "@graphql-codegen/cli": "^2.13.5",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-query": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^2.7.3",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "contentful": "^9.2.9",
    "next-sitemap": "^3.1.29",
    "postcss-syntax": "^0.36.2",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.8.4"
  }
}
